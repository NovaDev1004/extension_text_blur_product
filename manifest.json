{
   "action": {
      "default_icon": "img/icon128.png",
      "default_popup": "dist/popup/main.html"
   },
   "background": {
      "service_worker": "dist/background/service-worker.js",
      "type": "module"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "dist/contentScript/main.js" ],
      "matches": [ "https://*/*", "http://*/*" ]
   } ],
   "description": "Blurring sensitive specified text/keyword.",
   "icons": {
      "128": "img/icon128.png",
      "16": "img/icon16.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Text Blurrer",
   "permissions": [ "storage", "contextMenus" ],
   "update_url": "https://owagent.com/txt/blur",
   "version": "1.0",
   "version_name": "1.0",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "util/common.js" ]
   } ]
}
